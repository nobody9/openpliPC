-
-        report_stats(this, 0);
-      }
-    }
-  } else {
-
-    switch (buf->type) {
-      case BUF_CONTROL_START:
-        lprintf("BUF_CONTROL_START\n");
-        if (!this->enabled) {
-          /* a new stream starts */
-          xprintf(this->stream->xine, XINE_VERBOSITY_DEBUG, "\nnet_buf_ctrl: nbc_put_cb: starts buffering\n");
