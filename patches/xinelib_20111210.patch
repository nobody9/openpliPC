-    snd_mixer_close(this->mixer.handle);
-    this->mixer.handle=0;
-  }
-  pthread_mutex_destroy(&this->mixer.mutex);
-
-  if (this->audio_fd) snd_pcm_close(this->audio_fd);
-  this->audio_fd=NULL;
-  free (this);
